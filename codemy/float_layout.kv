<Button>
    font_size: 32
    size_hint: (0.3,0.3)

<MyLayout>
    FloatLayout:
        size: root.width, root.height

        Button:
            text: "top left"
            #{"x","y","top","left","right","top","bottom"}
            pos_hint: {"x":0, "top":1}

        Button:
            text: "top right"
            pos_hint: {"x":0.7, "top":1}


        Button:
            text: "center"
            pos_hint: {"x":0.35, "y":.35}



        Button:
            text: "bottom left"
            pos_hint: {"x":0}


        Button:
            text: "bottom right"
            pos_hint: {"x":0.7, "bottom":1}

        
            